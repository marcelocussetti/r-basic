    "import math\n",
    "rad=float(input(\"ingrese un valor en radianes: \"))\n",
    "\n",
    "def agrados(rad):\n",
    "    \n",
    "    enGrados=rad*180/math.pi\n",
    "    \n",
    "    print(F\"{rad} radianes, equivalen a: {round(enGrados,2)}°\")\n",
    "    \n",
    "    \n",
    "    return\n",
    "\n",
    "agrados(rad)\n"
   ]
